      "firebase.json",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
